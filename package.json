{
  "name": "react-dashboard",
  "version": "1.0.0",
  "description": "mern-stack dashboard",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node server/dist/server.js",
    "server": "concurrently \"tsc --watch\" \"nodemon -q server/dist/server.js\"",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client && npm start"
  },
  "author": "Abdulrhman Kotb",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  }
}
