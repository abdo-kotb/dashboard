{
  "version": 2,
  "builds": [
    {
      "src": "server/dist/server.js",
      "use": "@vercel/node"
    },

    {
      "src": "client/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "server/dist/server.js" }],
  "buildCommand": "npm run build --prefix client && tsc",
  "installCommand": "npm run install && npm run install --prefix client"
}
